PREFIX ext: <http://mu.semte.ch/vocabularies/ext/>

CONSTRUCT {
  ?s ?p ?o ;
    ext:wasInGraph ?g .
}
WHERE {
  VALUES ?type {
    <http://data.lblod.info/vocabularies/subsidie/climate/ClimateEntry>
  }

  GRAPH ?g {
    ?s a ?type ;
      ?p ?o .
  }

  FILTER(regex(STR(?g), "(http://mu.semte.ch/graphs/organizations/.*/LoketLB-subsidies)"))
}
